{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "react/jsx-filename-extension": [0],
    "sort-imports": [
      "off",
      "ignoreCase",
      "ignoreDeclarationSort",
      "ignoreMemberSort",
      "memberSyntaxSortOrder"
    ]
  }
}
